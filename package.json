    "file-saver": "^2.0.5",
    "js-base64": "^3.4.5",
    "masonry-layout": "^4.2.2",
    "ng-gallery": "^5.0.0",
    "ngx-infinite-scroll": "10.0.1",
    "ngx-trend": "^6.0.0",
    "ngx-webstorage": "7.0.1",
    "perfect-scrollbar": "^1.5.0",
