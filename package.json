    "@delon/acl": "11.7.0",
    "@delon/auth": "11.7.0",
    "@delon/cache": "11.7.0",
    "@delon/chart": "11.7.0",
    "@delon/form": "11.7.0",
    "@delon/mock": "11.7.0",
    "@delon/theme": "11.7.0",
    "@delon/util": "11.7.0",
    "@fortawesome/angular-fontawesome": "0.8.2",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
